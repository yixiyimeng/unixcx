<view class="content"><uni-nav-bar vue-id="1" title="{{title}}" bind:__l="__l" vue-slots="{{['left']}}"><view slot="left"><block wx:if="{{HomeData.NewMsg<=0}}"><view data-event-opts="{{[['tap',[['switchCity',['$event']]]]]}}" class="location" bindtap="__e"><image mode="widthFix" src="/static/images/address-icon.png"></image>{{''+cityName+''}}</view></block><block wx:if="{{HomeData.NewMsg>0}}"><navigator class="noReadMsg" hover-class="none" url="/pages/home/news/index"><image src="/static/images/noReadMsg.png"></image></navigator></block></view></uni-nav-bar><view class="searchWrap"><navigator class="searchBox" hover-class="none" url="/pages/sjjsAbout/searchResult"><image src="/static/images/searchHui.png"></image>输入关键字搜索</navigator></view><view class="wrap _div" style="{{'height:'+(scrollhight+'px')+';'+('top:'+(offsetTop+'px')+';')}}"><mescroll-uni vue-id="2" down="{{downOption}}" up="{{upOption}}" fixed="{{false}}" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]],['^init',[['mescrollInit']]]]}}" bind:down="__e" bind:up="__e" bind:init="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="swiper"><swiper style="height:100%;" autoplay="ture" interval="3000" duration="500" indicator-dots="true" indicator-active-color="#ffcc00" indicator-color="#fff" circular="true"><block wx:for="{{HomeData.Banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator hover-class="none" url="{{item.url}}"><swiper-item><image class="slide-image" src="{{item.Img}}"></image></swiper-item></navigator></block></swiper></view><view class="menu"><block wx:for="{{HomeData.BigButton}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="menu-item" url="{{item.url}}" hover-class="none"><view class="menu_img"><image class="slide-image" mode="widthFix" src="{{item.img}}"></image></view><view class="menu-tit">{{item.title}}</view></navigator></block></view><view class="today_info"><view class="date"><image mode="widthFix" src="/static/images/calendar_bg.png"></image><view class="d">{{intMont}}</view><view class="m">{{englistMont}}</view></view><view class="info"><view class="tit">{{HuangliInfo.date}}</view><view class="lc">{{HuangliInfo.lunar_year+" "+HuangliInfo.lunar_month+" "+HuangliInfo.lunar_day}}</view><view class="good"><view>宜</view>{{''+HuangliInfo.jishenyiqu+''}}</view><view class="bad"><view>忌</view>{{''+HuangliInfo.xiongshen+''}}</view></view></view><block wx:if="{{HomeData.ImgNews.length>0}}"><navigator class="newsBox" url="{{'/pages/sjjsAbout/news/details?id='+HomeData.ImgNews[0].Id}}" hover-class="none"><view class="left_img"><image mode="widthFix" src="{{HomeData.ImgNews[0].Img}}"></image></view><view class="newsRightInfo"><view class="news-title">{{HomeData.ImgNews[0].Title}}</view><view class="news-time">{{HomeData.ImgNews[0].time}}</view></view></navigator></block><block wx:if="{{HomeData.News&&HomeData.News.length>0}}"><view class="uni-swiper-msg "><view class="uni-swiper-msg-icon"><image src="/static/images/horn.png" mode="widthFix"></image></view><swiper class="flex-1" vertical="true" autoplay="true" circular="true" interval="3000"><block wx:for="{{HomeData.News}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator class="section" wx:key="key" url="`/pages/sjjsAbout/news/details?id=${item.Id}`" hover-class="none">{{item.Title}}</navigator></swiper-item></block></swiper></view></block><view class="list _div"><block wx:for="{{cemetery_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cemetery-info vue-id="{{('3-'+index)+','+('2')}}" cemetery-item="{{item}}" bind:__l="__l"></cemetery-info></block></view></mescroll-uni></view><mpvue-picker class="vue-ref" vue-id="4" themeColor="{{themeColor}}" mode="{{mode}}" deepLength="{{deepLength}}" pickerValueDefault="{{pickerValueDefault}}" pickerValueArray="{{pickerValueArray}}" data-ref="mpvuePicker" data-event-opts="{{[['^onConfirm',[['onConfirm']]],['^onCancel',[['onCancel']]]]}}" bind:onConfirm="__e" bind:onCancel="__e" bind:__l="__l"></mpvue-picker></view>